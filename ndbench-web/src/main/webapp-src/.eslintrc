{
    "env":{
        "browser":true,
        "es6":true
    },
    "extends": [
        "airbnb/base"
    ],
    "rules": {
        "indent":[2,4],
        "new-cap":0,
        "comma-dangle":0,
        "func-names":0,
        "no-var":0,
        "no-trailing-spaces":0,
        "eol-last":0,
        "object-shorthand":0,
        "prefer-arrow-callback":0,
        "prefer-template":0,
        "no-param-reassign": [2, {
            "props": false
        }]
    },
    "globals": {
        "Polymer": true
    }
}
